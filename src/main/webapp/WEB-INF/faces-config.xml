<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    
        
    <navigation-rule>
        <from-view-id>/bienvenida.xhtml</from-view-id>
        <navigation-case>
            <from-action>entrar</from-action>
            <to-view-id>/faces/login.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>   
    
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-action>newuser</from-action>
            <to-view-id>/faces/generar_usuario.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-action>consultaViajes</from-action>
            <to-view-id>/faces/consultaViajes.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-action>salir</from-action>
            <to-view-id>/faces/bienvenida.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>  
     
    <navigation-rule>
        <navigation-case>
            <from-view-id>/generar_usuario.xhtml</from-view-id>
            <from-action>login</from-action>
            <to-view-id>/faces/login.xhtml</to-view-id>
            <redirect/>  
        </navigation-case>      
    </navigation-rule> 
    
    <navigation-rule>
        <from-view-id>/consultaViajes.xhtml</from-view-id>
        <navigation-case>
            <from-action>empezar</from-action>
            <to-view-id>/faces/countryDates.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>  
        <navigation-case>
            <from-action>login</from-action>
            <to-view-id>/faces/login.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>  
    </navigation-rule> 
    
    <navigation-rule>
        <from-view-id>/countryDates.xhtml</from-view-id>
        <navigation-case>
            <from-action>equipajes</from-action>
            <to-view-id>/faces/equipajes.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>    
        <navigation-case>
            <from-action>salir</from-action>
            <to-view-id>/faces/login.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>      
    </navigation-rule>      
    
    <navigation-rule>
        <from-view-id>/equipajes.xhtml</from-view-id>
        <navigation-case>
            <from-action>a√±adir</from-action>
            <to-view-id>/faces/anyadirEquipaje.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>
        <navigation-case>
            <from-action>siguiente</from-action>
            <to-view-id>/faces/bestCountry.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>
        <navigation-case>
            <from-action>salir</from-action>
            <to-view-id>/faces/login.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/anyadirEquipaje.xhtml</from-view-id>
        <navigation-case>
            <from-action>insertar</from-action>
            <to-view-id>/faces/equipajes.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>
        <navigation-case>
            <from-action>volver</from-action>
            <to-view-id>/faces/equipajes.xhtml</to-view-id>
            <redirect/> 
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/bestCountry.xhtml</from-view-id>
        <navigation-case>
            <from-action>gastos</from-action>
            <to-view-id>/faces/gastosViaje.xhtml</to-view-id>
            <redirect/>           
        </navigation-case>       
    </navigation-rule>
        
      
</faces-config>
